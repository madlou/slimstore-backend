<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:mongo="http://www.springframework.org/schema/data/mongo"
    xsi:schemaLocation="http://www.springframework.org/schema/data/mongo http://www.springframework.org/schema/data/mongo/spring-mongo-1.0.xsd
                        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
                        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean class="com.tjx.lew00305.slimstore.barcode.BarcodeSpecification">
        <property name="region" value="EU" />
        <property name="banner" value="TKMAXX" />
        <property name="type" value="STORE" />
        <property name="length" value="16" />
        <property name="division" value="0" />
        <property name="department" value="2" />
        <property name="category" value="2" />
        <property name="style" value="6" />
        <property name="price" value="6" />
        <property name="week" value="0" />
    </bean>

    <bean class="com.tjx.lew00305.slimstore.barcode.BarcodeSpecification">
        <property name="region" value="EU" />
        <property name="banner" value="HOMESENSE" />
        <property name="type" value="STORE" />
        <property name="length" value="16" />
        <property name="division" value="0" />
        <property name="department" value="2" />
        <property name="category" value="2" />
        <property name="style" value="6" />
        <property name="price" value="6" />
        <property name="week" value="0" />
    </bean>

</beans>

